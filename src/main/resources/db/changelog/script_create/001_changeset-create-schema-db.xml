<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog logicalFilePath="db/changelog/changelog.xml"
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

  <!-- schema for liquibase -->
  <changeSet id="create-scheme-liquibase_schema" author="dg" context="local">
    <sql>
      CREATE SCHEMA IF NOT EXISTS liquibase_schema;
    </sql>
  </changeSet>
  <!-- schema for prm_service_reference -->
  <changeSet id="create-scheme-spec" author="dg" context="local">
    <sql>
      CREATE SCHEMA IF NOT EXISTS spec;
    </sql>
  </changeSet>
</databaseChangeLog>